<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <!-- ir.ui.menu inherit form view -->
    <record id="monolitic_ir_ui_menu" model="ir.ui.view">
        <field name="name">monolitic.ir.ui.form.view</field>
        <field name="model">ir.ui.menu</field>
        <field name="inherit_id" ref="base.edit_menu_access"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='web_icon_data']" position="after">
                <field name="url"/>
            </xpath>
        </field>
    </record>

</odoo>
